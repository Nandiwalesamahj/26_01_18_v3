<%= tinymce %>
<%= form_for @samajiksanstha do |f| %>
	<h4>शीर्षक</h4>
	<%= f.text_area :title,label: "शीर्षक" , class: 'tinymce' %>
	<h4>वर्णन</h4>
	<%= f.text_area :description,label: "वर्णन" , class: 'tinymce' %>
	<p>Attachment</p>
	<%= f.file_field :attachment,label: "add file" %>
	<%= f.submit "सबमिट करा" %>
<% end %>
